{
  "name": "tickets",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format:check": "prettier --plugin-search-dir . --check .",
    "format:write": "prettier --plugin-search-dir . --write .",
    "lint": "eslint .",
    "shadcn:add": "bunx shadcn-svelte@latest add",
    "db:studio": "drizzle-kit studio",
    "db:generate": "drizzle-kit generate:sqlite",
    "db:seed": "bun run src/lib/server/db/seed.ts",
    "db:migrate": "bun run db:generate && bun ./src/lib/server/db/migrate.ts"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.3.1",
    "@sveltejs/adapter-auto": "^2.1.1",
    "@sveltejs/kit": "^1.30.3",
    "@types/dom-view-transitions": "^1.0.4",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "autoprefixer": "^10.4.14",
    "better-sqlite3": "latest",
    "chalk": "^5.3.0",
    "drizzle-kit": "latest",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte": "^2.30.0",
    "postcss": "^8.4.24",
    "postcss-load-config": "^4.0.1",
    "prettier": "^3.1.1",
    "prettier-plugin-svelte": "^3.1.2",
    "svelte": "^4.2.8",
    "svelte-check": "^3.4.3",
    "svelte-headless-table": "^0.17.7",
    "tailwindcss": "^3.3.2",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^4.4.2"
  },
  "type": "module",
  "dependencies": {
    "@libsql/client": "^0.3.6",
    "@lucia-auth/adapter-sqlite": "^2.0.1",
    "@lucia-auth/oauth": "^3.5.0",
    "@paralleldrive/cuid2": "^2.2.2",
    "@react-email/components": "^0.0.12",
    "bits-ui": "^0.9.4",
    "clsx": "^2.0.0",
    "drizzle-orm": "^0.29.0",
    "formsnap": "^0.4.1",
    "lucia": "^2.7.3",
    "lucide-svelte": "^0.292.0",
    "mode-watcher": "^0.0.7",
    "react": "^18.2.0",
    "resend": "^2.0.0",
    "svelte-sonner": "^0.3.3",
    "sveltekit-superforms": "^1.10.1",
    "tailwind-merge": "^2.0.0",
    "tailwind-variants": "^0.1.18",
    "zod": "^3.22.4"
  }
}
